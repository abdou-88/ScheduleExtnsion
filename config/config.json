{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "schedule-extension-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/scheduleExtension/ScheduleExtensionApplicationCustomizer.js",
          "manifest": "./src/extensions/scheduleExtension/ScheduleExtensionApplicationCustomizer.manifest.json"
        }
      ]
    },
    "schedule-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/schedule/ScheduleWebPart.js",
          "manifest": "./src/webparts/schedule/ScheduleWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ScheduleExtensionApplicationCustomizerStrings": "lib/extensions/scheduleExtension/loc/{locale}.js",
    "ScheduleWebPartStrings": "lib/webparts/schedule/loc/{locale}.js"
  }
}
